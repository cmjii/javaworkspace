package day_10;

public class cardmain {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		CardPack cp = new CardPack();
		int index = 0;
//		cp.getPack()[0].print();
		for(int i =0; i<cp.getPack().length; i++) {
			cp.getPack()[i].print();
			index++;
			if(index%13==0) {
				System.out.println();
			}
		} 
		
	
		
	}

}
