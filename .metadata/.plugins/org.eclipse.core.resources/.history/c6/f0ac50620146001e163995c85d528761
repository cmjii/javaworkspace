package day_09;

public class 과제 {

	public static void main(String[] args) {
		// 클래스 객체 생성 (2명)
		// 값을 입력 후 정보출력 (2명)
		
		Studant stu = new Studant();
		
		stu.setName("홍길동");
		stu.setAge(21);
		stu.setNum("010-5555-6666");
		stu.print();
		
		Studant stu1 = new Studant();
		
		stu1.setName("심청이");
		stu1.setAge(25);
		stu1.setNum("010-9999-1111");
		stu1.print();
		
	}

}

//클래스 생성
//학생 클래스 생성
//이름, 나이, 전화번호, 주소
//출력
//이름(나이) 전화번호

class Studant {

	private String name;
	private int age;
	private String num;
	private String ad;
	
	public void print() {
		System.out.println(name+"("+age+")"+" "+num);
	}
	
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}


	public String getNum() {
		return num;
	}

	public void setNum(String num) {
		this.num = num;
	}

	public String getAd() {
		return ad;
	}

	public void setAd(String ad) {
		this.ad = ad;
	}


	
	

	
}